  },
  "bugs": {
    "url": "https://github.com/joshuaavalon/eslint-config/issues"
  },
  "author": "Joshua Avalon",
  "license": "Apache-2.0",
  "dependencies": {
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "standard-version": "^9.3.2"
  }
}
