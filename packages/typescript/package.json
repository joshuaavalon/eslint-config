{
  "name": "@joshuaavalon/eslint-config-typescript",
  "version": "7.0.0",
  "description": "Shareable ESLint Typescript config.",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c"
