{
  "name": "@joshuaavalon/eslint-config-react",
  "version": "10.0.0",
  "description": "Shareable ESLint React config.",
  "type": "module",
  "module": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "eslint",
    "typescript",
    "react"
  ],
  "files": [
    "dist"
  ],
  "author": "Joshua Avalon",
  "license": "Apache-2.0",
  "homepage": "https://github.com/joshuaavalon/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuaavalon/eslint-config.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/joshuaavalon/eslint-config/issues"
  },
  "dependencies": {
    "@joshuaavalon/eslint-config-typescript": "^10.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0"
  },
  "peerDependencies": {
    "eslint": "^9.37.0"
  }
}
